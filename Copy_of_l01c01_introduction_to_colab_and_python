        "reviews_df = pd.DataFrame()\n",
        "\n",
        "path_reviews = '//div[@data-hook=\"review\"]'\n",
        "reviews = bre.xpath(path_reviews)\n",
        "\n",
        "path_rating  = './/i[@data-hook=\"review-star-rating\"]//text()'\n",
        "path_title   = './/a[@data-hook=\"review-title\"]//text()'\n",
        "path_author  = './/span[@class=\"a-profile-name\"]//text()'\n",
        "path_date    = './/span[@data-hook=\"review-date\"]//text()'\n",
        "path_body    = './/span[@data-hook=\"review-body\"]//text()'\n",
        "path_helpful = './/span[@data-hook=\"helpful-vote-statement\"]//text()'\n",
        "\n",
        "for review in reviews:\n",
        "    rating  = review.xpath(path_rating)\n",
        "    title   = review.xpath(path_title)\n",
        "    author  = review.xpath(path_author)\n",
        "    date    = review.xpath(path_date)\n",
        "    body    = review.xpath(path_body)\n",
        "    helpful = review.xpath(path_helpful)\n",
        "\n",
        "    review_dict = {'rating': rating,\n",
        "                   'title': title,\n",
        "                   'author': author,\n",
        "                   'date': date,\n",
        "                   'body': body,\n",
        "                   'helpful': helpful}\n",
        "    reviews_df = reviews_df.append(review_dict, ignore_index=True)\n",
        "\n",
        "reviews_df.to_csv(\"amazon_reviews.csv\", sep='\\t', encoding='utf-8')\n",
        "print(reviews_df)\n"
